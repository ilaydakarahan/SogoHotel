@model List<HotelListViewModel.Hotel>
@{
    Layout = null;
}

<!DOCTYPE html>

<h1>otel aranınca çıkan sayfa</h1>
<table>
    <tr>
        <th>otel adı</th>
    </tr>
    @foreach (var item in Model)
    {
        <div class="col-lg-4 col-md-6">
            <div class="room-item">
                <img src="@item.property.photoUrls[0].Replace("square60","square480")" alt="">
                <div class="ri-text">
                    <h4>@item.property.name</h4>
                    <h3>@item.property.priceBreakdown.excludedPrice.value.ToString("0") $<span></span></h3>
                    <table>
                        <tbody>
                            <tr>
                                <td>@item.accessibilityLabel</td>
                            </tr>

                        </tbody>
                    </table>
                
                </div>
            </div>
        </div>
    }


</table>